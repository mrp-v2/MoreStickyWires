{
  "multipart": [
    {
      "apply": {
        "model": "morestickywires:block/slime_center_multilayer"
      }
    },
    {
      "when": {
        "OR": [
          {
            "north": "none",
            "south": "none",
            "east": "none",
            "west": "none"
          },
          {
            "AND": [
              {
                "OR": [
                  {
                    "north": "side|up"
                  },
                  {
                    "south": "side|up"
                  }
                ]
              },
              {
                "OR": [
                  {
                    "east": "side|up"
                  },
                  {
                    "west": "side|up"
                  }
                ]
              }
            ]
          }
        ]
      },
      "apply": {
        "model": "morestickywires:block/infiniwire_dot"
      }
    },
    {
      "when": {
        "north": "side|up"
      },
      "apply": {
        "model": "morestickywires:block/infiniwire_north"
      }
    },
    {
      "when": {
        "east": "side|up"
      },
      "apply": {
        "model": "morestickywires:block/infiniwire_east"
      }
    },
    {
      "when": {
        "south": "side|up"
      },
      "apply": {
        "model": "morestickywires:block/infiniwire_south"
      }
    },
    {
      "when": {
        "west": "side|up"
      },
      "apply": {
        "model": "morestickywires:block/infiniwire_west"
      }
    },
    {
      "when": {
        "north": "side|up",
        "south": "side|up",
        "east": "none",
        "west": "none"
      },
      "apply": {
        "model": "morestickywires:block/infiniwire_north_south"
      }
    },
    {
      "when": {
        "east": "side|up",
        "west": "side|up",
        "north": "none",
        "south": "none"
      },
      "apply": {
        "model": "morestickywires:block/infiniwire_east_west"
      }
    },
    {
      "when": {
        "north": "up"
      },
      "apply": {
        "model": "morestickywires:block/sticky_infiniwire_up"
      }
    },
    {
      "when": {
        "east": "up"
      },
      "apply": {
        "model": "morestickywires:block/sticky_infiniwire_up",
        "y": 90
      }
    },
    {
      "when": {
        "south": "up"
      },
      "apply": {
        "model": "morestickywires:block/sticky_infiniwire_up",
        "y": 180
      }
    },
    {
      "when": {
        "west": "up"
      },
      "apply": {
        "model": "morestickywires:block/sticky_infiniwire_up",
        "y": 270
      }
    }
  ]
}